{
  "name": "logic-flow-for-react",
  "version": "0.0.11",
  "scripts": {
    "bootstrap": "yarn clean && lerna bootstrap",
    "clean": "lerna clean -y",
    "createLib": "cd packages && lerna create",
    "start": "yarn  && dumi dev",
    "docs:build": "dumi build",
    "deploy": "bash deploy.sh",
    "build": "lerna run build",
    "build:h": "lerna run build --scope=@logic-flow-react/hooks",
    "build:u": "lerna run build --scope=@logic-flow-react/utils",
    "build:c": "lerna run build --scope=@logic-flow-react/components",
    "build:f": "lerna run build --scope=logic-flow-react",
    "pub": "lerna publish"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.14.0",
    "@logicflow/core": "^1.1.22",
    "@logicflow/extension": "^1.1.22",
    "@umijs/test": "^3.0.5",
    "antd": "^4.22.3",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.1.46",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lerna": "^3.22.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "rc-resize-observer": "^1.2.0",
    "yorkie": "^2.0.0"
  },
  "license": "MIT"
}
